{
  "name": "frontend-mentor-personal-finance-app-be",
  "version": "1.0.0",
  "description": "Personal Finance App Backend - NestJS with Bun Runtime",
  "main": "dist/src/main",
  "scripts": {
    "build": "nest build",
    "start": "bun run dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "bun run dist/src/main.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:e2e": "bun test test/e2e",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "seed": "bun run src/cli.ts seed",
    "seed:clear": "bun run src/cli.ts seed:clear"
  },
  "keywords": [
    "nestjs",
    "bun",
    "postgresql",
    "drizzle",
    "personal-finance"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@sentry/node": "^7.92.0",
    "@sentry/tracing": "^7.92.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^7.1.0",
    "drizzle-orm": "^0.29.3",
    "express-session": "^1.17.3",
    "nest-commander": "^3.19.0",
    "postgres": "^3.4.3",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "^1.2.21",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "drizzle-kit": "^0.20.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
